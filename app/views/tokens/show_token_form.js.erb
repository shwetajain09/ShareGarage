modal = $('#app_modal');
modal.on('show.bs.modal', function (event) {
	modal.find('.modal-title').text('Redeem Token');
	modal.find('.modal-body').html("<%= escape_javascript(render(:partial => 'redeem_token_form')) %>");
	modal.find('.modal-footer').html('<button type="button" class="btn btn-default" data-dismiss="modal">Close</button>');

})

$('#app_modal').modal('show');