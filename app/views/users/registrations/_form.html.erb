        <div class="row ">
            <div class="col-xs-12 ">
                <div class="panel panel-default">
                    <!-- <div class="panel-heading">
                        <h3 class="panel-title text-center">Please Register</h3>
                    </div> -->
                    <div class="panel-body">
                        <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
                            <%= devise_error_messages! %>
                        <form role="form">
                            <div class="form-group">
                                <%= f.text_field :first_name, autofocus: true,:class=>"form-control input-sm", :placeholder=>"First Name" %>
                            </div>

                            <div class="form-group">
                                <%= f.text_field :last_name, autofocus: true,:class=>"form-control input-sm", :placeholder=>"Last Name" %>
                            </div>

                            <div class="form-group">
                                <%= f.email_field :email, autofocus: true,:class=>"form-control input-sm", :placeholder=>"Email Address" %>
                            </div>

                            <div class="row">
                                <div class="col-xs-6 col-sm-6 col-md-6">
                                    <div class="form-group">
                                        
                                        <%= f.password_field :password, autocomplete: "off",:class=>"form-control input-sm", :placeholder=>"Password" %>
                                        <% if @minimum_password_length %>
                                            <em>(<%= @minimum_password_length %> characters minimum)</em>
                                        <% end %>
                                    </div>
                                </div>
                                <div class="col-xs-6 col-sm-6 col-md-6">
                                    <div class="form-group">
                                        
                                        <%= f.password_field :password_confirmation, autocomplete: "off",:class=>"form-control input-sm", :placeholder=>"Confirm Password"%>
                                    </div>
                                </div>
                            </div>
                            <%= f.submit "Sign up" ,:class=>"btn btn-info btn-block"%>
                        <% end %>

                <%= render "devise/shared/links" %>
                    </div>
                </div>
            </div>
        </div>
