        <div class="row ">
            <div class="col-xs-12 ">
                <div class="panel panel-default">
                    <!-- <div class="panel-heading">
                        <h3 class="panel-title text-center">Please Register</h3>
                    </div> -->
                    <div class="panel-body">
                        <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
                            <%= devise_error_messages! %>
                        <form role="form">
                            <div class="form-group">
                                <%= f.text_field :first_name, autofocus: true,:class=>"form-control input-sm", :placeholder=>"First Name" %>
                            </div>

                            <div class="form-group">
                                <%= f.text_field :last_name, autofocus: true,:class=>"form-control input-sm", :placeholder=>"Last Name" %>
                            </div>

                            <div class="form-group">
                                <%= f.email_field :email, autofocus: true,:class=>"form-control input-sm required", :placeholder=>"Email Address *" %>
                            </div>

                            <div class="row">
                                <div class="col-xs-6 col-sm-6 col-md-6">
                                    <div class="form-group">
                                        
                                        <%= f.password_field :password, autocomplete: "off",:class=>"form-control input-sm required", :placeholder=>"Password *" %>
                                        <em>(8 characters minimum)</em>
                                       
                                    </div>
                                </div>
                                <div class="col-xs-6 col-sm-6 col-md-6">
                                    <div class="form-group">
                                        
                                        <%= f.password_field :password_confirmation, autocomplete: "off",:class=>"form-control input-sm required", :placeholder=>"Confirm Password *"%>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-xs-6 col-sm-6 col-md-6">
                                        <div class="form-group required">
                                            <label class="radio-inline"><input type="radio" name="user[gender]" value="M" ><%= image_tag '/assets/girl.jpeg' %></label>
                                            <label class="radio-inline"><input type="radio" name="user[gender]" value="F" ><%= image_tag '/assets/boy.jpeg' %></label>
                                            
                                        </div>
                                </div>
                                
                            </div>
                            <div class="row">
                                <div class="col-xs-6 col-sm-6 col-md-6">
                                        <div class="form-group">
                                            <%= f.text_field :phone_no, autofocus: true,:class=>"form-control input-sm", :placeholder=>"Mobile Number" %>
                                            
                                        </div>
                                </div>
                                <div class="col-xs-6 col-sm-6 col-md-6">
                                    <div class="form-group">
                                      <%= f.check_box :show_phone %>
                                      <%= f.label :show_phone %>
                                    </div>
                                </div>
                                <%= f.hidden_field :latitude %>
                                <%= f.hidden_field :longitude %>
                               <!--  <input type="hidden" id="userlat" name = "startlat" />
<input type="hidden" id="userlongitude" name = "startlon" />
 -->

                            </div>
                            <%= f.submit "Sign up" ,:class=>"btn btn-info btn-block"%>
                        <% end %>
                        <br>
                        <%- if controller_name != 'sessions' %>
                          <p class="text-center">Already a member? <%=link_to 'Login',show_modal_home_path('login'),:remote => true%></p>
                        <% end -%>
                <%#= render "users/shared/links" %>
                    </div>
                </div>
            </div>
        </div>
<script>
    if (navigator.geolocation) {
      console.log('Geolocation is supported!');
    }
    else {
      console.log('Geolocation is not supported for this Browser/OS version yet.');
    }
    $(function(){
        var startPos;
        var geoOptions = {
          enableHighAccuracy: true
        }

        var geoSuccess = function(position) {
          startPos = position;
          $('#user_latitude').val(startPos.coords.latitude);
          $('#user_longitude').val(startPos.coords.longitude);
          console.log(startPos.coords.latitude);
           console.log(startPos.coords.longitude);
        };
        var geoError = function(error) {
          console.log('Error occurred. Error code: ' + error.code);
          // error.code can be:
          //   0: unknown error
          //   1: permission denied
          //   2: position unavailable (error response from location provider)
          //   3: timed out
        };

        console.log(navigator.geolocation.getCurrentPosition(geoSuccess, geoError, geoOptions));
  });
</script>